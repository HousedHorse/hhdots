[options]
resize_grip = false
scroll_on_output = false
scroll_on_keystroke = true
audible_bell = false
visible_bell = false
mouse_autohide = false
allow_bold = true
dynamic_title = true
urgent_on_bell = true
clickable_url = true
font = Inconsolata-dz for Powerline 12
scrollback_lines = 1000
search_wrap = true
cursor_blink = system
cursor_shape = block
browser = firefox

[colors]

# special
foreground      = #e3d9d3
foreground_bold = #e3d9d3
cursor          = #e3d9d3
background      = #111110
background = rgba(17, 17, 16, 0.95)

# black
color0  = #111110
color8  = #9e9793

# red
color1  = #965e30
color9  = #965e30

# green
color2  = #926f5c
color10 = #926f5c

# yellow
color3  = #ae8b72
color11 = #ae8b72

# blue
color4  = #8f7e80
color12 = #8f7e80

# magenta
color5  = #ae9a92
color13 = #ae9a92

# cyan
color6  = #cbb3a4
color14 = #cbb3a4

# white
color7  = #e3d9d3
color15 = #e3d9d3

# types color
#color121 = #91a34c
#color81  = #91a34c

[hints]
#font = Monospace 12
#foreground = #dcdccc
#background = #3f3f3f
#active_foreground = #e68080
#active_background = #3f3f3f
#padding = 2
#border = #3f3f3f
